    "url": "https://github.com/TeamlifyLLC/json-env-parser/issues"
  },
  "homepage": "https://github.com/TeamlifyLLC/json-env-parser#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@vercel/ncc": "^0.38.1",
    "dotenv": "^16.4.5",
    "parse-env-string": "^1.0.1",
    "prettier": "^3.3.3",
    "vitest": "^2.1.1",
    "xo": "^0.59.3"
